{
  "title": "Booking resource",
  "description": "Booking resource object",
  "type": "object",
  "properties": {
    "id": {
      "description": "Booking id",
      "type": "integer"
    },
    "first_name": {
      "description": "First name",
      "type": "string",
      "minLength": 1,
      "maxLength": 255
    },
    "last_name": {
      "description": "Last name",
      "type": "string",
      "minLength": 1,
      "maxLength": 255
    },
    "date_of_birth": {
      "description": "Date of birth",
      "type": "string",
      "format": "date-time"
    },
    "checkin_datetime" : {
      "description": "Check-in date time",
      "type" : "string",
      "format" : "date-time",
      "customDateTimePattern" : "yyyy-MM-dd HH:mm:ss"
    },
    "checkout_datetime" : {
      "description": "Check-out date time",
      "type" : "string",
      "format" : "date-time",
      "customDateTimePattern" : "yyyy-MM-dd HH:mm:ss"
    },
    "totalprice": {
      "description": "Total price",
      "type" : "number"
    },
    "deposit": {
      "description": "Deposit amount",
      "type": "number"
    },
    "address": {
      "description": "Address",
      "type": "object",
      "properties": {
        "line1": {
          "description": "Address line 1",
          "type": "string"
        },
        "line2": {
          "description": "Address line 2",
          "type": "string"
        },
        "city": {
          "description": "City",
          "type": "string"
        },
        "state": {
          "description": "State",
          "type": "string"
        },
        "zip_code": {
          "description": "Zip code",
          "type": "string"
        }
      },
      "required": ["line1", "line2", "city","state" ,"zip_code"]
    }
  },
  "required": [
    "first_name",
    "last_name"
  ]
}